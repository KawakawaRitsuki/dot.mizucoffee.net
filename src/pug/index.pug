doctype html
html(lang='ja')
  -
    const info = {
      title: "dot.mizucoffee.net",
      description: "dot.mizucoffee.net"
    }
  - data = data.filter((e) => !e.disable)
  head
    title=info.title
    script(src='./bundle.js')
    link(rel='stylesheet', href='./style.css', type='text/css')
    include head
  body
    div#welcome Welcome to
    div.hideable.clearfix#head-box
      div#roulette
        - let roulette = data.concat()
        div.item.item-show= roulette[0].subdomain
        - roulette.shift()
        each item in roulette
          div.item.item-before= item.subdomain
      div.domain .mizucoffee.net
    h1.hideable#label subdomain
    div.hideable#list-box
      each item in data
        div.hideable.subdomain= item.subdomain
      div.hideable#list-domain .mizucoffee.net
//      a(href="#").base
    footer &copy; 2018 mizucoffee.net
